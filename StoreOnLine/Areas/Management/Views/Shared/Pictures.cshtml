@model IEnumerable<StoreOnLine.DataBase.Model.Resources.Imagen>

<div id="cPictures" class="carousel slide">
    <div class="carousel-inner">
        @foreach (var img in Model)
        {
            <div class="item">
                <img class="img-thumbnail" width="50" height="50" src="@Url.Action("GetImageProductBase", "AdminImg", new { Area = "Management", id = @img.Id })" alt=""/>
                <div class="carousel-caption">
                    <h4>@img.ObjectName</h4>
                </div>
            </div>
        }

    </div>
</div>

