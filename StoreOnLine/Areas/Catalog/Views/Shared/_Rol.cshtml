@model IEnumerable<CategoryRol>

<div class="col-lg-10">
    <table id="DataTableCategory" class="table table-striped ">
        <thead>
            <tr>
                <th></th>
                <th>Id</th>
                <th>Rol</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td><input type="checkbox"/></td>
                    <td>@item.Id</td>
                    <td>@item.Rol.Name</td>
                </tr>
            }
        </tbody>
    </table>
</div>
